{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "jsx": "react",
    "strict": true,
    "strictFunctionTypes": false,
    "declaration": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "stripInternal": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "paths": {
      "@pulsar-ui/core": ["./node_modules/@pulsar-ui/core"],
      "@pulsar-ui/core/*": ["./node_modules/@pulsar-ui/core/*"],
      "@pulsar-ui/system": ["./node_modules/@pulsar-ui/system"],
      "@pulsar-ui/system/*": ["./node_modules/@pulsar-ui/system/*"],
      "@pulsar-ui/palette": ["./node_modules/@pulsar-ui/palette"],
      "@pulsar-ui/palette/*": ["./node_modules/@pulsar-ui/palette/*"]
    }
  }
}
